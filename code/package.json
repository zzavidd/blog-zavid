{
  "name": "zavid-blog",
  "packageManager": "yarn@3.2.2",
  "stylelint": {
    "extends": "@zzavidd/stylelint-config"
  },
  "prettier": "@zzavidd/prettier-config",
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "check": "tsc --noEmit",
    "check:test": "tsc --project ./test/tsconfig.test.json",
    "manual": "ts-node --project test/tsconfig.test.json test/manual",
    "compile": "tsc --project tsconfig.override.json",
    "ingestion": "ts-node --project test/tsconfig.test.json test/ingestion",
    "test": "ts-mocha --project test/tsconfig.test.json --recursive --no-timeout test/suite.ts",
    "test:unit": "ts-mocha --project test/tsconfig.test.json --recursive --no-timeout test/unit/**/*.test.ts",
    "test:service": "ts-mocha --project test/tsconfig.test.json --recursive --no-timeout test/service/**/*.test.ts",
    "eslint": "eslint --ext .ts,.tsx src/ --cache",
    "prettier:check": "prettier --ignore-path ../.gitignore --check \"**/*.{js,ts,tsx,scss,json,html}\"",
    "prettier:write": "yarn run prettier:check --write",
    "prune": "ts-prune -p ./tsconfig.json -s **/*.ts"
  },
