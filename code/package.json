{
  "scripts": {
    "build": "NODE_ENV=development next build",
    "build:ci": "next build",
    "check": "tsc --noEmit --project ./tsconfig.json",
    "check:classes": "tsc --noEmit --project ./classes/tsconfig.classes.json",
    "check:test": "tsc --project ./test/tsconfig.test.json",
    "dev": "nodemon",
    "preprod": "tsc --project tsconfig.override.json",
    "prod": "NODE_ENV=production node src/server.js",
    "ingestion": "ts-node test/ingestion",
    "test": "npm run test:unit && npm run test:service",
    "test:unit": "ts-mocha --project test/tsconfig.test.json --recursive --no-timeout test/unit/**/*",
    "test:service": "ts-mocha --project test/tsconfig.test.json --recursive --no-timeout test/service/**/*",
    "test:manual": "ts-node --project test/tsconfig.test.json test/manual",
    "lint": "eslint --ignore-path ../.gitignore \"src/**\"",
    "prettify": "prettier --ignore-path ../../.gitignore --write \"**/*.js\""
